    "compilerOptions": {
        "composite": true,
        "target": "ES2020",
        "lib": [
            "ES2020"
        ],
        "module": "commonjs",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "strict": true,
        "outDir": "dist-electron"
    },
    "include": [
        "electron"
    ]
}