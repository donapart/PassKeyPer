{
    "name": "@passkeyper/io",
    "version": "1.0.0",
    "description": "Import/Export functionality for PassKeyPer",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "test": "vitest run",
        "test:watch": "vitest",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@passkeyper/core": "*",
        "papaparse": "^5.4.1"
    },
    "devDependencies": {
        "@types/papaparse": "^5.3.14",
        "tsup": "^8.0.1",
        "typescript": "^5.3.0",
        "vitest": "^1.0.4"
    }
}